<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.5.xsd">

    <changeSet id="3-drop-temp-public-tables" author="tokman">
        <preConditions onFail="MARK_RAN">
            <tableExists schemaName="public" tableName="databasechangelog"/>
            <tableExists schemaName="public" tableName="databasechangeloglock"/>
        </preConditions>
        <sql>
            DROP TABLE IF EXISTS public.databasechangeloglock CASCADE;
            DROP TABLE IF EXISTS public.databasechangelog CASCADE;
        </sql>
    </changeSet>

</databaseChangeLog>